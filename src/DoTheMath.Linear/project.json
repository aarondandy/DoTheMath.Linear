{
  "version": "0.0.0-*",
  "description": "A bunch of stuff for Linear Algebra.",
  "authors": [ "aarondandy" ],
  "copyright": "Copyright (c) 2016 Aaron Dandy",
  "packOptions": {
    "tags": [
      "linear",
      "algebra",
      "math",
      "vector",
      "matrix"
    ],
    "owners": [ "aarondandy" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/aarondandy/DoTheMath.Linear"
    },
    "licenseUrl": "https://github.com/aarondandy/DoTheMath.Linear/blob/master/LICENSE",
    "projectUrl": "https://github.com/aarondandy/DoTheMath.Linear",
    "files": {
      "exclude": [
        "**/*.targets"
      ]
    },
    "requireLicenseAcceptance": false
  },
  "buildOptions": {
    "compile": {
      "includeFiles": [
        "../../SharedAssemblyInfo.cs"
      ],
      "excludeFiles": [
        "DoTheMath.Linear.nuget.targets"
      ]
    },
    "define": [ "TRACE" ],
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ]
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ],
        "optimize": false
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE" ],
        "optimize": true,
        "warningsAsErrors": true
      }
    }
  },
  "frameworks": {
    "net35": {
      "buildOptions": {
        "define": [ "PRE_NETSTANDARD", "NET_3_5", "PRE_CORE" ]
      }
    },
    "net451": {
      "buildOptions": {
        "define": [ "NETSTANDARD_1_2", "NET_4_5_1", "HAS_CODECONTRACTS", "PRE_CORE" ]
      }
    },
    "net461": {
      "buildOptions": {
        "define": [ "NETSTANDARD_1_4", "NET_4_6_1", "HAS_CODECONTRACTS", "PRE_CORE" ]
      }
    },
    "netstandard1.0": {
      "buildOptions": {
        "define": [ "NETSTANDARD_1_0" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027"
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "define": [ "NETSTANDARD_1_1" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027"
      }
    },
    "netstandard1.2": {
      "buildOptions": {
        "define": [ "NETSTANDARD_1_2" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027"
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETSTANDARD_1_3" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027"
      }
    },
    "netstandard1.4": {
      "buildOptions": {
        "define": [ "NETSTANDARD_1_4" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027"
      }
    },
    "netstandard1.5": {
      "buildOptions": {
        "define": [ "NETSTANDARD_1_5" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027"
      }
    }
  }
}
