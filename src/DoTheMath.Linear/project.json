  "licenseUrl": "https://github.com/aarondandy/DoTheMath.Linear/blob/master/LICENSE",
  "compile": [
    "../../SharedAssemblyInfo.cs"
  ],
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "optimize": false
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "warningsAsErrors": true
      }
    }
  },
  "runtimes":{
    "win": { }  
  },
  "frameworks": {
    "net35": {
      "compilationOptions": {
        "define": [ "PRE_NETSTANDARD", "NET_3_5" ]
      }
    },
    "net451": {
      "compilationOptions": {
        "define": [ "NETSTANDARD_1_2", "NET_4_5_1" ]
      }
    },
    "net461": {
      "compilationOptions": {
        "define": [ "NETSTANDARD_1_3", "NET_4_6" ]
      }
    },
    "dotnet5.4": {
      "compilationOptions": {
        "define": [ "NETSTANDARD_1_3", "NET_4_6" ]
      },
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-beta-23516",
        "System.Collections": "4.0.11-beta-23516",
        "System.Linq": "4.0.1-beta-23516",
        "System.Runtime": "4.0.21-beta-23516"
      }
    }
  }
